{
  "name": "electronupdater",
  "author": "Mega Conecta Soluções em Tecnologia <desenvolvimento@megaconecta.com.br>",
  "version": "v1.0.11",
  "main": "./public/electron.js",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.13",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Guilherme-j10/electron_autoupdatertest.git"
  },
  "build": {
    "appId": "com.test.electronupdate",
    "productName": "Cloud Call - Agente",
    "win": {
      "target": "nsis",
      "icon": "./build/megaconecta.png"
    },
    "linux": {
      "target": "deb"
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "scripts": {
    "dev": "concurrently \"npm run start\" \"wait-on http://localhost:3234 && electron .\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dist": "electron-builder --publish=never",
    "deploy": "electron-builder -p always",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.18",
    "concurrently": "^6.4.0",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "wait-on": "^6.0.0"
  }
}
